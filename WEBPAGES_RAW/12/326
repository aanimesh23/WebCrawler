<script type="text/javascript">
$(document).ready(function() {
						
	var annualReports = $("#annualReports");
	
	var image = $(".image");
	var informationContainer = $(".informationContainer");
	var information = $(".information");
	var container = $("#container");
	
	annualReports.hide();
	
	information.hide();
	
	//initially show annualreports
	annualReports.delay("300").animate({ //allows fade in and slide down at the same time
											"height": "toggle"
											}, "fast");
	
	$("#annualReportsMenu").removeClass("unselected").addClass("selected");

   //if they click the NSF image it tells you what it is
   image.hover(function() {
						   information.fadeIn("slow");
				  },
				  function() {
					  	   information.fadeOut("slow");
				  }
	);
   
   //End Submenu
});
</script>

