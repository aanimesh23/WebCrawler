<script type="text/javascript">
$(document).ready(function() {
						 
	var presentations = $("#presentations");
	
	var image = $(".image");
	var informationContainer = $(".informationContainer");
	var information = $(".information");
	var container = $("#container");
	
	presentations.hide();
	
	information.hide();
	
	//initially show annualreports
	presentations.delay("300").animate({ //allows fade in and slide down at the same time
											"height": "toggle", "opacity": "toggle"
											}, "normal");
	
	$("#presentationsMenu").removeClass("unselected").addClass("selected");
   
   //if they click the NSF image it tells you what it is
   image.hover(function() {
						   information.fadeIn("slow");
				  },
				  function() {
					  	   information.fadeOut("slow");
				  }
	);
   
   //End Submenu
});
</script>

