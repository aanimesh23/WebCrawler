	<script type="text/javascript">
    // check QuickLink form for not being empty
    function VerifyQLForm() {
        var QLQueryHelp = "Enter a word or phrase";
        if (document.forms.QL.q.value == '' || document.QL.q.value == QLQueryHelp) {
            document.forms.QL.q.value = QLQueryHelp;
            document.forms.QL.q.select();
            alert('It appears that you clicked on the "Search" button but forgot to enter a UCI keyword. Please enter a word or phrase and try again.')
            return false;
        } else {
            document.forms.QL.q.value = "site:www.studyabroad.uci.edu " + document.forms.QL.q.value;
            return true;
        }
    }
    </script>

