<SCRIPT LANGUAGE="JavaScript">
<!-- Begin

function bannerObject(p,d)
{
  this.msg   = MESSAGE
  this.out   = " "
  this.pos   = POSITION
  this.delay = DELAY
  this.i     = 0
  this.reset = clearMessage
}

function clearMessage()
{
  this.pos = POSITION
}

var POSITION = 50
var DELAY    = 100
var MESSAGE  = " Welcome to the JavaScript Sweden site. I'm doing my very best to keep this site"
             + " updated with all the new JavaScript features and examples I can find."
             + " Please send me all the great JavaScript stuff you can find out there."
             + " I hope you will send me some mail if you have any ideas on how to improve this site. "
var scroll = new bannerObject()

function scroller()
{
  for (scroll.i = 0; scroll.i < scroll.pos; scroll.i++)
  {
    scroll.out += " "
  }
  
  if (scroll.pos >= 0)
   scroll.out += scroll.msg
  else scroll.out = scroll.msg.substring(-scroll.pos,scroll.msg.length)

  document.bannerform.banner.value = scroll.out

  scroll.out = " "
  scroll.pos--
  
  if (scroll.pos < -(scroll.msg.length))
  {
    scroll.reset()
  }
  
  setTimeout ('scroller()',scroll.delay)
}

// End -->
</SCRIPT>
</HEAD>
<BODY BACKGROUND="bg2.gif" onload="scroller()">
