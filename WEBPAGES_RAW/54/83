   <!-- /bin/inc/course_info/listing.inc.php -->   

<!-- SELECT distinct Schedule.year, Courses.course_id,  Courses.department,  Courses.number,  Courses.title,  Courses.description, UCInetID, quarter, quantity FROM Schedule INNER JOIN Courses ON  (Schedule.course_id = Courses.course_id or Courses.course_id in (select course_id from crosslist x where x.crossgroup in (select crossgroup from crosslist where course_id =Schedule.course_id) and x.course_id!=Schedule.course_id)) WHERE Schedule.year = '0'  AND exists (select 1 from published p where p.year=Schedule.year and p.quarter=Schedule.quarter and p.department=Courses.department) ORDER BY department asc, number asc -->
<!--

<!DOCTYPE html PUBLIC '-//W3C//DTD XHTML 1.1//EN'
