<H2><A NAME="SECTION0026710000000000000000">
23.7.1 OEReadFASTAFile</A>
</H2>

<P>
<dl><dd><pre class="verbatim">bool MyReadFASTAMolecule(oemolistream &amp;ifs, OEMolBase &amp;mol)
{
  mol.Clear();
  if (OEReadFASTAFile(ifs,mol))
  {
    OEFindRingAtomsAndBonds(mol);
    OEAssignAromaticFlags(mol);
    return true;
  }
  mol.Clear();
  return false;
}
</pre></dl>

<P>

