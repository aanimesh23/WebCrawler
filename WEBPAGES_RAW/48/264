<Script Language="JavaScript1.1">
var Cited = new Array()
var CitedBy = new Array()
Cited["maximum"] = 0

for (var i in Citations) {
    for (var j in Citations[i]) {
         if (Cited[Citations[i][j]]) {
	     Cited[Citations[i][j]] += 1
	     if (Cited[Citations[i][j]] > Cited["maximum"])
	         {Cited["maximum"] = Cited[Citations[i][j]]}}
	     else {Cited[Citations[i][j]] = 1}
	 if (CitedBy[Citations[i][j]])
	    {CitedBy[Citations[i][j]][CitedBy[Citations[i][j]].length] = i}
	    else {CitedBy[Citations[i][j]]= new Array()
	          CitedBy[Citations[i][j]][0]=i}
	  }
     }



function windowsp() {
 var a =navigator.userAgent;
 for (var i = 0; i <(a.length - 3); i++) {
      if (a.substring(i, i+3) == "Win")
          {return true}
	  }
 return false}

var UnderWin32p = windowsp()

	 
</Script>

