function login() {
  // XXX We have to use the index, because I can't figure out how 
  // to get the option value!
  role = document.forms[0].elements["role"].options.selectedIndex;

  //  alert ("role: " + role )
  if (role == 0) {
    top.location = "proposal/top.html";
  } else if (role == 1) {
    top.location = "pre-awd/top.html";
  } else if (role == 2) {
    top.location = "grant-awd/top.html";
  } else if (role == 3) {
    top.location = "grant-awd/top.html";
  }
}
  </script>
