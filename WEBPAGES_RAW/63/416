        <script type="text/javascript">
			function toggle_visibility(obj_id){
	            var obj = $("#"+obj_id+"-wrapper");
	            if ( obj.is(":visible") ){
					// Hide content
	            	obj.hide();
	            	// Remove transparent layer
	            	$('#transparent-layer').hide();
	            }
	            else{
					// Hide everything else
					$('#example-wrapper').hide();
					$('#data-sources-wrapper').hide();
					$('#help-wrapper').hide();
					$('#contact-wrapper').hide();

		            // Show content
	            	obj.show();

	            	// Add transparent layer
	            	$('#transparent-layer').show();
	            }
			}

            /***************************************/
            // Define Global Vars
            /***************************************/
            var node_json_datas = Array();
            /***************************************/

            // Set and return options depending on what type (table/chart)
            function setOptions(type, colors, title){
                var options = {
                    enableEvents: true,
                    enableInteractivity: true,
                    focusTarget: 'category',
                    series: colors,
                    allowHtml: true,
                    legend: {
                        position: 'none'
                    }
                }
                if (type == "chart"){
                	options['title'] = title;
                	options['titleTextStyle'] = {fontSize: 20}
                    options['width'] = 500;
                    options['height'] = 300;
                    options['vAxis'] = {title: 'Scaled Intensity'};
                    options['hAxis'] = {title: 'Time'};
                    options['curveType'] = 'function';

                }
                else if(type == "table"){
                    options['width']=900;
                    options['height']=null;
                }
                return options;
            }

            // Draw chart, view can be google visualization data or a view
            function drawChart(view, chart_elements, colors, title) {
                // Create and draw the visualization
                $.each(chart_elements, function(index,chart_element){
                    gchart = new google.visualization.LineChart(chart_element);
                    options = setOptions("chart", colors, title);
                    gchart.draw(view, options);
                });
            }

            // Define toggle visible chart lines function
            function toggleLines(gtable_id, data, columns, colors, title){
                var table_id = gtable_id.split("-");
                var chart_id = "chart-" + table_id[1];

                // Create copy of data as view
                var chartdata = new google.visualization.DataTable(data);
                var chartview = new google.visualization.DataView(chartdata);
                var hidden_cols = new Array();
                var color_idxs = new Array();
                for (column in columns){
                    // Add line data index for hidding
                    col_id = parseInt(columns[column])
                    hidden_cols.push(col_id);
                    color_idxs.push( (col_id-1) / 3 );

                    // Add interval data index for hidding
                    hidden_cols.push(col_id + 1 );
                    hidden_cols.push(col_id + 2 );
                }

                // Parse colors and only copy over visible colors
                var visible_colors = Array();
                for (color in colors){
                    if ($.inArray(parseInt(color),color_idxs) < 0){
                        visible_colors.push(colors[color]);
                    }
                }

                // Hide unchecked columns
                chartview.hideColumns(hidden_cols);
                // Draw chart with only visible colors and hidden columns
                drawChart(chartview, $.find('#'+chart_id+":first"), visible_colors, title);
            }

            $(document).on("change", "#tissue", function () {
                var bioelement = $("#bioelement").val();
                var tissue = $(this).val();
                $(".paper-wrapper").each(function() { $(this).css("display", "none"); });
                if ($("#" + bioelement + "-" + tissue + "-paper").length!=0) {
                  $("#" + bioelement + "-" + tissue + "-paper").css("display", "block");
                } else {
                  $("#" + tissue + "-paper").css("display", "block");
                }
                if (tissue != "all") { $("#search-img").removeClass("disabled"); $("#submit").removeClass("disabled"); }
                else { $("#search-img").addClass("disabled"); $("#submit").addClass("disabled"); }
                $("#select_tissue").hide();
            });

            function renderElement(){
                var bioelement = $(this).val();
                $("#tissue").find("option").each(
                  function (i, x) {
                    $("#tissue option[value='"+x.value+"']").attr("disabled", "disabled");
                  });
                $("#tissue").find("."+bioelement).each(
                  function (i, x) {
                    $("#tissue option[value='"+x.value+"']").removeAttr("disabled");
                  });
                var selected = $("#tissue").find(":selected").first();
                if (selected.val() != "all" && !selected.hasClass(bioelement)) {
                  var first_valid_tissue = $("#tissue").find("."+bioelement).first().val();
                  $("#tissue").val(first_valid_tissue);
                }
                $("#tissue").trigger("change");
            };

            $(document).on("change", "#bioelement", renderElement);
            $(document).on("ready", renderElement);

            // Draw table with all data (table data and corresponding chart data) and all colors
            function drawTable(json_data, table_elements, colors, title) {
                // Init data object
                var data = new google.visualization.DataTable(json_data.tabledata);

                // Create and draw the visualization
                $.each(table_elements, function(index,table_element){
                    // Make a working copy of data and hide the index column
                    var view = new google.visualization.DataView(data);
                    // Hide index column
                    view.hideColumns([1]);

                    // Draw the table with view and options
                    var options = setOptions("table", colors, "");
                    var gtable = new google.visualization.Table(table_element);
                    gtable.draw(view, options);

                    google.visualization.events.addListener(gtable, 'select', function() {
                        // Grab row that was clicked and toggle Plot boolean
                        selection = gtable.getSelection();
                        if (selection.length > 0){
                            var row = selection[0].row
                            if (data.getValue(row, 0) == false){
                                data.setValue(row, 0, true);
                            }
                            else{
                                data.setValue(row, 0, false);
                            }

                            // Make a working copy of data
                            var view = new google.visualization.DataView(data);
                            // Hide the index column
                            view.hideColumns([1]);

                            var hidden_cols = new Array;
                            for (var i=data.getNumberOfRows(); i>0; i--){
                                if (!data.getValue(i-1, 0)){
                                    // Add line data index for hiding
                                    hidden_cols.push(data.getValue(i-1, 1));
                                }
                            }

                            // Define additional table options
                            var options = setOptions("table", colors, "");

                            // Re-Draw table with view and options (losses any previous column sort!)
                            gtable.draw(view, options);
                            toggleLines($(table_element).attr("id"), json_data.chartdata, hidden_cols, colors, title);
                        }
                    });
                });
            }

            // Print out all the meta data into the meta DOM element
            function drawMeta(data, meta_elements, comp_id){
                $.each(meta_elements, function(index, meta_element){
                    $.each(data, function(key, value){
                    	var index = value[0];
                    	var item = value[1];
                    	if (index=="networkurl"){
                        	if (item.length > 0){
                                var networkurl_elements = $(meta_element).find(".networkurl");
                                $(networkurl_elements).eq(0).append("<a target='_blank' href='"+ item +"'>View Network</a>");
                        	}
                        }
                        else{
                            var meta_p_elements = $(meta_element).find("p");
                            $(meta_p_elements).eq(0).append("<label>"+index+"</label>: "+item+"<br />"); }
                    });
                });
            };

            // $("#biocyclebutton").click(function() {
            //   $("#biocyclefrm").submit();
            // });

            // When DOM elements are ready...
            $(document).ready(function() {
                google.setOnLoadCallback(function() {
                    $(function() {
                    	//BrowserDetect.init();
                    	detectBrowserVersion();
                        $("#blank_search").hide();
                        $("#select_tissue").hide();

                    	//Dissable tissue on live site
                    	if (false){
                    		if ( 'None' == 'muscle' ){
                    			$("#bioelement option[value='acetylome']").hide();
                    			$("#bioelement option[value='gene']").hide();
                    			// Disable "all" option
                    			$("#tissue option[value='all']").attr("disabled","disabled");
                    			// Hide "liver" option
                    			$("#tissue option[value='liver']").hide();
                    		}
                    		else if ( 'None' == 'acetylome' ){
                    			// Disable "all" option
                    			$("#tissue option[value='all']").attr("disabled","disabled");
                    			// Hide "muscle" option
                    			$("#tissue option[value='muscle']").hide();
                    			// Show crick link to acetylome network
                    			$("#acetylome_network").show();
                    		}
                    		else if ( 'None' == 'highfat' ){
                    			$("#bioelement option[value='acetylome']").hide();
                    		}
                    	    else{
                    	    	//Do not hide anything
                    	    }
                    	}
                    	else{
                    		//$("#tissue option[value='muscle']").attr("disabled","disabled");
                    		$("#logout_link").hide();
                    	}

                    	// Set selected bioelement
                        $("#bioelement").val('gene');
                        // Set selected tissue
                        if ('all' != "all")
                           $("#submit").removeAttr("disabled");
                        $("#tissue").val('all');
                        $("#bioelement").trigger("change");
                        $("#tissue").trigger("change");
                        if ("all" == "all") { $("#search-img").addClass("disabled"); $("#submit").addClass("disabled"); }

                        function initNodeData(element) {
                            var comp_id = element.attr("id");
                            $.getJSON('/?comp_id='+comp_id+"&bioelement="+$('#bioelement').val()+"&tissue="+$('#tissue').val(), function(data) {
                                // Init data object

                                // Draw Chart
                                var chartdata = new google.visualization.DataTable(data.chartdata);
                                var chart_element = element.children(".chart").eq(0);
                                google.setOnLoadCallback(drawChart(chartdata, chart_element, data.colors, data.name));

                                // Draw Table
                                var table_element = element.children(".table").eq(0);
                                google.setOnLoadCallback(drawTable(data, table_element, data.colors, data.name));

                                // Draw Meta data
                                var meta_element = element.children(".meta").eq(0);
                                google.setOnLoadCallback(drawMeta(data.metadata, meta_element, comp_id));
                            }).fail(function () {
                                element.hide()
                                console.log(element.attr("id") + " failed to load node data.");
                            });

                            return true
                        }
                        $('#bioelement').change(function() {
                            switch($(this).attr('value')) {
                                case "gene":
                                    $("#element_search_text" ).text("Search transcriptome by official gene symbol or REFSEQ");
                                    break;
                                case "metabolite":
                                    $("#element_search_text" ).text("Search metabolome by bioechemical name or KEGG ID");
                                    break;
                                case "acetylome":
                                    $("#element_search_text" ).text("Search acetylome by official gene symbol or REFSEQ");
                                    break;
                                case "proteome":
                                    $("#element_search_text" ).text("Search proteome by official gene symbol");
                                    break;
                            }
                        });
                        $( "#submit" ).click(function() {
                            console.log("submit1");
                            if ($("#search-img").hasClass("disabled")) {
                                if(!($('#select_tissue').is(":visible"))) {
                                    $('#select_tissue').toggle();
                                }
                            } else if($("#search").attr('value') == "") {
                                if(!($('#blank_search').is(":visible"))) {
                                    $('#blank_search').toggle();
                                }
                            } else {
                                document.searchfrm.submit();
                            }
                        });
                        $(document).on("submit", function () {
                            console.log("submit2");
                            if ($("#search-img").hasClass("disabled")) {
                                if(!($('#select_tissue').is(":visible"))) {
                                    $('#select_tissue').toggle();
                                }
                                return false;
                            } else if($("#search").attr('value') == "") {
                                if(!($('#blank_search').is(":visible"))) {
                                    $('#blank_search').toggle();
                                }
                                return false;
                            } else {
                                return true;
                            }
                        });
                        // Bind toggle to menu links
                        $("#menu-links a").click(function(){
                        	var this_class = $(this).attr('class');
                        	toggle_visibility(this_class);
                        });

                        // Initialize each node by pulling data and displaying charts
                        $(".node-data").each(function(index, node){
                            var success = initNodeData($(node));
                        	// Hide the loading gif after everything has been loaded
                            if (success){
                            	$(node).children(".loading").eq(0).hide();
                            }
                        });

                        // Initialize autocomplete on search input
                        $( "#search" ).autocomplete({
                            url: '/?get=search-vals',
                            minChars: 2,
                            useCache: true,
                            maxCacheLength: 5,
                            remoteDataType: "json",
                            queryParamName: "search",
                            filterResults: true,
                            matchInside: false,
                            maxItemsToShow: 1000,
                            maxCacheLength: 5,
                            extraParams: {"bioelement":'gene'}
                        });
                        var ac = $("#search").data("autocompleter");

                        // Set extra parameter for autocomplete to pass via AJAX call
                        $('#bioelement').change(function() {
                            ac.setExtraParam('bioelement', $('#bioelement').val());

                            if ($('#bioelement').val() != 'Select bioelement'){
                                $('#search').removeAttr('disabled');
                            }
                            else{
                                $('#search').attr('disabled', true);
                            }
                        });
                	});
                });
            });
        </script>

