<script>
$(function(){
  var create_new_view_dialog = $("#create-new-view-dialog");
  if (create_new_view_dialog[0])
    create_new_view_dialog.dialog({
      autoOpen: false,
      height: "auto",
      width: "auto",
      modal: true,
      position: { my: "top",
                  at: "top+200",
                  of: window },
      close: function() {
        $("#create-new-view-layer").toggle();
        $("#create-new-view-confirmation-layer").html("");
	newViewDialogCloseCallback();
      }
    });
});
</script>

