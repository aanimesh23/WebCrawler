            var lastSliderSubMenu = null;
            function showSubMenu( ele ) {
                if( lastSliderSubMenu != null ) {
                    if( ele != lastSliderSubMenu ) {
                        $( lastSliderSubMenu ).siblings( 'ul' ).slideToggle();
                        $( lastSliderSubMenu ).html( "+" );
                    }
                }
                
                $( ele ).html( ( lastSliderSubMenu == ele ? "+" : "&#8212;" ) );
                $( ele ).siblings( 'ul' ).slideToggle();
                lastSliderSubMenu = ele != lastSliderSubMenu ? ele : null;
                return false;
            }
        </script>
