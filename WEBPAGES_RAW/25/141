@inproceedings{Mat-JNM-76,
  title = {{Branch and bound clique determination by breadth-minimum search}},
  author = {D. W. Matula},
  booktitle = {Proc. ORSA/TIMS Joint Nat. Mtg.},
  publisher = {TIMS/ORSA Bulletin},
  pages = {147},
  year = {1976},
  abstract = {A branch and bound procedure is described for determining the
    size of the largest complete subgraph of a graph. A breadth-minimum search
    of a graph entails the sequential extraction of a minimum degree vertex
    in the remaining graph. It is shown that utilizing breadth-minimum search
    as a branching strategy significantly reduces the size of the subproblems
    generated during the branch and bound implicit enumeration. A variety
    of bounding techniques is utilized. The expected growth rate of this
    algorithm (on random graphs) is at a rate much better than exponential
    though not polynomial bounded. Practically, problems as large as 150
    to 200 vertices with edge densities up to 50{\%} should generally be tractable
    by this procedure. Computational experience is discussed.}}

@article{MatTho-DM-92,
  title = {{On the complexity of finding iso- and other morphisms for partial
    $k$-trees}},
