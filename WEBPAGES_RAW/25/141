  year = {1981},
  abstract = {Describes an algorithm which lists all cliques of a planar
    graph in linear time.}}

@article{PapYan-JACM-82,
  title = {{The complexity of restricted spanning tree problems}},
  author = {Christos H. Papadimitriou and M. Yannakakis},
  journal = {J. Assoc. Comput. Mach.},
  volume = {29},
  pages = {285--309},
  year = {1982},
  abstract = {The complexity of the following class of problems is investigated:
    given a distance matrix, find the shortest spanning tree that is isomorphic
    to a given prototype. Several classical combinatorial problems, both
    easy and hard, fall into this category for an appropriate choice of the
    family of prototypes; for example, taking the family to be the set of
    all paths gives the traveling salesman problem or taking the family to
    be the set of all 2-stars gives the weighted matching problem. It is
    shown that the complexity of these problems depends explicitly on the
    rate of growth of a simple parameter of the family of prototypes.}}

@article{PeaCaeBis-PR-94,
  title = {{Rulegraphs for graph matching in pattern recognition}},
  author = {A. Pearce and T. Caelli and W. F. Bischof},
  journal = {Pattern Recognition},
  volume = {27},
  pages = {1231--1247},
  year = {1994},
  abstract = {In pattern recognition, the graph matching problem involves
    the matching of a sample data graph with the subgraph of a larger model
    graph where vertices and edges correspond to pattern parts and their
    relations. In this paper, we present rulegraphs, a new method that combines
    the graph matching approach with rule-based approaches from machine learning.
    This new method reduces the cardinality of the (NP-complete) graph matching
    problem by replacing model part, and their relational attribute states,
    by rules which depict attribute bounds and evidence for different classes.
    We show how rulegraphs, when combined with techniques for checking feature
    label-compatibilities, not only reduce the search space but also improve
    the uniqueness of the matching process.}}

@inproceedings{Pen-NSM-94,
  title = {{Neural network architecture for object recognition}},
  author = {M. Peng},
  booktitle = {Neural and Stochastic Methods in Image and Signal Processing
    III},
  number = {2304},
  series = {Proc. SPIE},
  publisher = {Int. Soc. for Optical Engineering},
  pages = {225--232},
  year = {1994},
  abstract = {Model-based 2-D object recognition is investigated by using
    a neural network. Object recognition is treated as a subgraph matching.
    A neural network system is proposed to complete subgraph matching. The
    system consists of a large Hopfield network, called a global network,
    and several small Hopfield networks, called subnetworks. The system starts
    with a randomly set initial state of the global network. The subnetworks
    are dynamically created based on the stable output state of the global
    network and then the outputs of the subnetworks are fed back to the global
    network to reset its initial state. This process continues till the whole
    system is stabilized, where the optimal subgraph matching is obtained.
    This method avoids the local minimum problem from using a single Hopfield
    network and also uses much less calculation time than the simulated annealing
    algorithm. Computer simulation is done to verify operation of the system.}}

@inproceedings{PenGup-ICNN-94,
  title = {{Occluded object recognition by Hopfield networks}},
  author = {M. Peng and N. K. Gupta},
  booktitle = {Proc. Int. Conf. Neural Networks},
  publisher = {Inst. Electrical {\&} Electronics Engineers},
  volume = {7},
  pages = {4309--4315},
  year = {1994},
  abstract = {A new method to use a Hopfield neural network for object recognition
    is proposed. Object recognition is treated as a subgraph matching. A
    system consisting of one global network and several sub-networks is constructed.
    The sub-networks are dynamically changed and the outputs of the global
    network and the sub-networks are fedback to each other to complete the
    subgraph matching. This method avoids the local minimum problem arising
    from the use of one single Hopfield network and it also uses much less
    time than the simulated annealing algorithm. Computer simulation shows
    it can efficiently recognize objects in occlusion.}}

@article{PenGup-IJEEE-95,
  title = {{Invariant and occluded object recognition based on graph matching}},
  author = {M. Peng and N. K. Gupta},
  journal = {Int. J. Electrical Engineering Education},
  volume = {32},
  pages = {31--38},
  year = {1995},
  abstract = {In this paper, an neural network algorithm to recognise objects,
    which are in occlusion and may have changes in position, rotation and
    scale compared with their models, is proposed. A polygonal approximation
    is used to represent the object's boundary and angles of vertices as
    the features. A new relation, characterising spatial information between
    vertices, is introduced. The subgraph matching is achieved by a simulated
    annealing algorithm. The paper aims to generate project ideas for final
    year students at university and also to provide a basis for further research.}}

@article{Per-Cyb-84,
  title = {{A class of multicriterial problems on graphs and hypergraphs}},
  author = {V. A. Perepelitsa},
  journal = {Cybernetics},
  volume = {20},
  pages = {542--548},
  year = {1984},
  abstract = {The discrete optimization problems considered in this article
    arise in connection with the development of CAD systems for the electronics
    industry. The definitions of the relevant technical terms will be found
    in (1-3). The mathematical formulation of the multicriterial problem
    of electronic circuit assembly covers two stages of the design process:
    (1) covering the devices or the logical circuits by standard units (microcircuit
    modules); (2) cutting the overall electric circuit obtained in the preceding
    stage into the electric circuits of the basic components. In CAD practice,
    the two stages are usually called the 'covering problem' and the 'partition
    problem'. The covering problem has been formulated in graph-theoretical
    terms for the case of units with unconnected elements, when there is
    no need to recognize subgraph isomorphism and the article considers the
    covering of a graph by fixed-length chains. The partition problem is
    formulated in hypergraph terms and it only deals with the cutting of
    the original circuit into subcircuits, without considering subcircuit
    placement, i.e. without connecting the elements to switching contacts.}}

@inproceedings{PerChrCop-ISCS-92,
  title = {{An exact algorithm for the technology fitting problem in the
    application specific state machine device}},
  author = {M. A. Perkowski and M. Chrzanowska-Jeske and A. Coppola and E.
    Pierzchala},
  booktitle = {Proc. Int. Symp. Circuits and Systems},
  publisher = {Inst. Electrical {\&} Electronics Engineers},
  volume = {4},
  pages = {1977--1980},
  year = {1992},
  abstract = {The fitting problem for a new application-specific state machine
    device, CY7C361, from Cypress Semiconductor is formulated, and a solution
    is proposed. This fitting problem consists of mapping the netlist obtained
    from high-level synthesis into the chip's physical resources. In general,
    the mapping (fitting) problem can be formulated as one of the labeled
    graph isomorphism between the netlist graph and the subgraph of the resources
    graph. However, the specific architecture-related constraints of the
    CY7C361 device cause the fitting problem to be generalized as a graph
    isomorphism problem with some additional mapping constraints. The formulation
    is quite general for a class of electronically programmable logic device
    (EPLD) fitting problems. An exact, constraint-based, tree searching algorithm
    with several kinds of backtracking was implemented.}}

@inproceedings{PleVoi-WG-90,
  title = {{Finding minimally weighted subgraphs}},
  author = {J. Plehn and B. Voigt},
  booktitle = {Proc. 16th Int. Worksh. Graph-Theoretic Concepts in Computer
    Science},
  number = {484},
  series = {Lecture Notes in Computer Science},
  publisher = {Springer-Verlag},
  pages = {18--29},
  year = {1991},
  abstract = {Let $H$ be a fixed graph of tree-width $w$. The authors study
    the problems of finding a minimally weighted weak $H$-subgraph, resp.,
    a minimally weighted induced $H$-subgraph in an edge-weighted graph $G$
    on $n$ nodes. They present an $O(n^{w+1})$-algorithm for the weak subgraph
    problem. If the maximal degree of $G$ is bounded, the algorithm above
    can be modified to an $O(n^{w+1})$-algorithm for the induced subgraph
    problem.}}

@inproceedings{PoiRicTij-ECCE-90,
  title = {{An object-oriented semantic description of procedures for evaluation
    of interfaces}},
  author = {S. Poitrenaud and J. F. Richard and C. A. Tijus},
  booktitle = {Proc. 5th Eur. Conf. Cognitive Ergonomics},
  pages = {431--440},
  year = {1990},
  abstract = {The central issue of evaluating interfaces with a new approach
    of declarative and procedural knowledge representation is addressed.
    The authors demonstrate how knowledge involved in using a system can
    be represented into semantic networks. In such semantic networks, objects
    are instances of classes and subclasses represented as nodes of a directed
    graph where the vertices indicate an inclusion relation structure which
    is defined by the procedures to which they may be applied. From the analysis
    of two different technical devices, a new telephone terminal and a text-editor,
    the cognitive relevance of this approach as a model for human knowledge
    representation is shown: by representing and simulating within this model
    the behavior of subjects learning elementary device functions and by
    analyzing the 'cognitive complexity' of a system. For a device and a
    novice, comparing and evaluating graphs or subgraph isomorphism of semantic
    networks provide a good prediction of learning difficulties. For two
    given devices, a good prediction of differences in cognitive complexity
    is also provided. Course of learning is described by the enrichment and
    the restructuring of a semantic network that is first related to a familiar
    device which stands as a source for analogical transfer and learning
    appears to be the progressive construction of a semantic network which
    is consistent with the device to be used.}}

@inproceedings{Por-LATIN-92,
  title = {{Even induced cycles in planar graphs}},
  author = {O. Porto},
  booktitle = {Proc. 1st Latin Amer. Symp. Theoretical Informatics},
  number = {583},
  series = {Lecture Notes in Computer Science},
  publisher = {Springer-Verlag},
  pages = {417--429},
  year = {1992},
  abstract = {An $O(n^3)$ algorithm for testing the existence of even induced
    cycles in planar graphs is presented. Even induced cycles are closely
    related to $W$-Perfection. A graph is $W$-Perfect if $W(H)= \chi (H)$,
    For all $H$ contained in $G$, $H$ induced subgraph of $G$, $\chi (H)$
    chromatic number of $H$, $W(G)=\max(\delta (H)+1,H$ contained in $G)$.
    The algorithm presented is based on decomposing a given planar graph
    $G$ through two and three cuts, adding artificial edges and vertices
    to the graphs obtained after each cut in order to retain the induced
    cycle parity of $G$ and, finally, if no even cycle was found during the
    cut operations, a set of graphs with no special cuts is obtained. There
    is no even induced cycle in the original graph if each graph in that
    set is isomorphic to any member of a set of 16 small graphs. Otherwise,
    there is an even induced cycle in the original graph $G$, which can be
    located by a backtracking operation. The complexity of testing the existence
    of even induced cycles in a general graph is still unknown.}}

@inproceedings{ProSte-GST-93,
  title = {{The Asymptotic Structure of $H$-free Graphs}},
