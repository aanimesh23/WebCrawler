@article{Oza-IECEJ-78,
  title = {{O-subgraph isomorphism of triply connected planar graphs}},
  author = {T. Ozawa},
  journal = {Trans. Inst. Electronics and Communication Engineers of Japan
    E},
  volume = {E61},
  pages = {96},
  year = {1978},
  abstract = {Summary form only given, substantially as follows: A planar
    graph $G$ is said to be an O-subgraph of another planar graph $G'$, if
    $G$ can be embedded in $G'$, that is, every mesh of $G$ except one called
    the outer mesh has a corresponding mesh in $G'$. The outer mesh of $G$
    may not be known, which makes the problem of the subgraph isomorphism
    difficult. The algorithm to test O-subgraph isomorphism consists of two
    parts. One of them determines, starting from an edge $e_1$ of $G$ and
    an edge $e_1'$ of $G'$, the correspondence between the edges of $G$ and
    those of $G'$ by use of an ordered quadruple consisting of the lengths
    of the meshes on the right and the left of edge $e$ and of the degrees
    of the tail and the head of $e$. The other part of the algorithm selects
    the starting pair of edges.}}

@article{PapYan-IPL-81,
  title = {{The clique problem for planar graphs}},
  author = {Christos H. Papadimitriou and M. Yannakakis},
  journal = {Information Processing Letters},
  volume = {13},
