@inproceedings{LiuMa-ICIAS-90,
  title = {{3-d object recognition by graph matching}},
  author = {Y. C. Liu and S. D. Ma},
  booktitle = {Proc. Intl. Conf. Intelligent Autonomous Systems},
  volume = {2},
  pages = {918--921},
  year = {1990},
  abstract = {A model-based method for recognizing 3D objects from their
    2D projections is presented. A 3D object is modelled by object graph
    where nodes correspond to object surfaces and edges correspond to object
    boundaries. The 2D projection of a 3D object is modelled by image graph
    where nodes correspond to image regions and edges correspond to image
    boundaries. Some geometrical and physical properties are assigned to
    nodes and edges in object graphs and image graphs. Since the images graph
    is a subgraph isomorphism of its associated object graph, the 3D objects
    can be recognized by subgraph matching. The subgraph matching algorithm
    is established on the basis of Hopfield neural network model.}}

@article{LoePol-JCTB-93,
  title = {{Efficient subgraph packing}},
  author = {M. Loebl and S. Poljak},
  journal = {J. Combinatorial Theory, Series B},
  volume = {59},
  pages = {106--121},
  year = {1993},
  abstract = {The authors are concerned with a natural generalization of
    the matching problem: the packing of graphs from a pregiven family. They
    characterize completely the complexity of deciding the existence of a
    perfect packing, if the pregiven family consists of two graphs, one of
    which is one edge on two vertices. The results complete the work of A.
    Cornuejols, D. Hartvigsen, and W. R. Pulleyblank (1982) and P. Hell and
    O. Kirkpatrick (1983).}}

@inproceedings{MagNat-ICRA-87,
  title = {{Hypothesizing undetected and occluded three-dimensional features
    using predicate logic based spatial reasoning}},
  author = {M. Magee and M. Nathan},
  booktitle = {Proc. Int. Conf. Robotics and Automation},
  publisher = {Inst. Electrical {\&} Electronics Engineers},
  volume = {2},
  pages = {935--940},
  year = {1987},
  abstract = {A method for hypothesizing the locations of features that may
    have been undetected by low-level three-dimensional image processing
    algorithms is presented. The procedure relies on a predicate logic based
    approach to object recognition in which viewpoint dependent (observed)
    data are transformed into a set of viewpoint-independent assertions.
    The detected features in these viewpoint-independent assertions are variabilized
    and the assertions are combined to form a clause which is tested for
    consistency against known models. If the features in a known model are
    able to instantiate the free variables in the variabilized clause, then
    a subgraph isomorphism is established between the features in the observed
    data and those of the model. Missing or undetected features in the observed
    data are then sought by examining the semantic relationships between
    model features that are instantiated and those that are not. Results
    of applying the method to synthetic range data are presented.}}

@article{Mak-IPL-89,
  title = {{On the subtree isomorphism problem for ordered trees}},
  author = {E. Makinen},
  journal = {Information Processing Letters},
  volume = {32},
  pages = {271--273},
  year = {1989},
  abstract = {The author deals with subtree isomorphism problem: Given two
    rooted trees $T_1$ and $T_2$, decide whether $T_1$ is isomorphic to any
    subtree of $T_2$. A tree is said to be ordered if the relative order
    of its subtrees in each node is fixed. Otherwise, a tree is unordered.
    The subtree isomorphism problem is studied in the case of unordered trees.
    If $T_1$ has $m$ nodes and $T_2$ has $n$ nodes then there is a $O(mn^{1.5})$
    time algorithm for the subtree isomorphism problem. He shows that a $O(m+n)$
    time algorithm can be obtained when restricting himself to ordered trees
    only. The algorithm is based on tree encoding and on string pattern matching.}}

@inproceedings{MarSho-CER-89,
  title = {{Abbreviated query interpretation in extended entity-relationship
    oriented databases}},
  author = {V. M. Markowitz and A. Shoshani},
  booktitle = {Proc. 8th Int. Conf. on Entity-Relationship},
  pages = {40--58},
  year = {1989},
  abstract = {The paper presents a methodology to support the expression
    of abbreviated queries in extended entity-relationship (EER) oriented
    relational databases, that is, relational databases whose schemas are
    translations of EER schemas. In EER-oriented databases, abbreviated queries
    can be represented by disconnected subgraphs of the corresponding EER
    diagrams. A criterion is proposed for determining the connected subgraphs
    corresponding to such abbreviated queries. The criterion is based on
    the robustness of the relational view corresponding to a connected subgraph
    of an EER diagram. The paper also investigates techniques that take advantage
    of specific properties of EER diagrams in order to reduce the complexity
    of finding the subgraphs that correspond to a given abbreviated query.}}

@article{MasNai-IPL-92,
  title = {{Deciding whether graph $G$ has page number one is in NC}},
  author = {S. Masuyama and S. Naito},
  journal = {Information Processing Letters},
  volume = {44},
  pages = {7--10},
  year = {1992},
  comment = {I thought parallel outerplanar recognition was long since known,
    e.g. I think we cited some paper on it in \cite{ChrEpp-TCS-91}, and it's
    not greatly different from also much-studied series parallel recognition.
    Maybe I should look this up to see if the bounds are better. DE},
  abstract = {Based on a forbidden subgraph characterization of a graph to
    have one page, the authors develop a polylog time algorithm to tell if
    a given graph G has page number one, with polynomial number of processors,
    clarifying this problem to be in NC. A graph G has page number one if
    there exists a linear arrangement of vertices so that no pair of edges
    is crossing when they are drawn on the same side of the linear arrangement
    of the vertices.}}

@article{Mat-ADM-78,
  title = {{Subtree isomorphism in $O(n^{5/2})$}},
  author = {D. W. Matula},
