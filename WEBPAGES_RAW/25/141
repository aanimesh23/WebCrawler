@article{CheDon-PRAI-91,
  title = {{A graph matching approach to 3-D point correspondences}},
  author = {J.-C. Cheng and H.-S. Don},
  journal = {Int. J. Pattern Recognition and Artificial Intelligence},
  volume = {5},
  pages = {399--412},
  year = {1991},
  abstract = {A recursive descent tree traversal algorithm is presented to
    find the point correspondences between two views. Given two sets of noisy
    3-D feature points on multiple rigid objects at two time-sequential views,
    the points at the first view are constructed as a graph and then another
    graph is derived from those feature points at the second view so that
    a maximal matching point and minimum matching error is obtained. The
    correspondence of the vertices is then found. The algorithm can also
    be used when the number of points at two views are different. Therefore,
    after matching, the occluded points at either view or both views can
    be identified. A data set splitting strategy for such cases, which can
    significantly reduce the computation time, is presented. Another algorithm
    presented is one in which motion parameters are estimated from a matched
    subgraph and are then used to guide the matching for the rest of the
    nodes.}}

